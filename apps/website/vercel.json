{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "routes": [
    {
      "src": "^/pricing/?$",
      "destination": "/preise/",
      "status": 301
    },
    {
      "src": "^/about/?$",
      "destination": "/uber-uns/",
      "status": 301
    },
    {
      "src": "^/contact/?$",
      "destination": "/kontakt/",
      "status": 301
    },
    {
      "src": "^/categories/auto/?$",
      "destination": "/auto-fahrschule/",
      "status": 301
    },
    {
      "src": "^/categories/motorrad/?$",
      "destination": "/motorrad-fahrschule/",
      "status": 301
    },
    {
      "src": "^/categories/lastwagen/?$",
      "destination": "/lastwagen-fahrschule/",
      "status": 301
    },
    {
      "src": "^/categories/taxi/?$",
      "destination": "/taxi-fahrschule/",
      "status": 301
    },
    {
      "src": "^/faq/?$",
      "destination": "/",
      "status": 301
    },
    {
      "src": "^/kategorie-a/?$",
      "destination": "/motorrad-fahrschule/",
      "status": 301
    },
    {
      "src": "^/kategorie-b/?$",
      "destination": "/auto-fahrschule/",
      "status": 301
    },
    {
      "src": "^/kategorie-be/?$",
      "destination": "/anhaenger-fahrschule/",
      "status": 301
    },
    {
      "src": "^/kategorie-c/?$",
      "destination": "/lastwagen-fahrschule/",
      "status": 301
    },
    {
      "src": "^/kategorie-d/?$",
      "destination": "/bus-fahrschule/",
      "status": 301
    },
    {
      "src": "^/kategorie-bpt/?$",
      "destination": "/taxi-fahrschule/",
      "status": 301
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
